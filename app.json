{
  "expo": {
    "name": "missle-wars",
    "slug": "missle-wars",
    "scheme": "missle-wars",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/logo.gif",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/logo.png",
      "backgroundColor": "#ffffff"
    },
    "extra": {
      "dotenv": ".env",
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "a596b2af-22b1-49e1-9e3c-714016198194"
      }
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.longtimenoc.misslewars"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/logo.gif",
        "backgroundColor": "#ffffff"
      },
      "package": "com.longtimenoc.misslewars"
    },
    "web": {
      "favicon": "./assets/logo.png"
    },
    "plugins": [
      "expo-router"
    ]
  }
}
