{
  "expo": {
    "name": "missile-wars",
    "slug": "missilewars",
    "scheme": "missile-wars",
    "version": "0.0.3",
    "orientation": "portrait",
    "icon": "./assets/logo.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/logo.png",
      "backgroundColor": "#ffffff"
    },
    "extra": {
      "dotenv": ".env",
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "250fd3fc-22b2-411d-abfc-63f3a6b81328"
      }
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "buildNumber": "2",
      "bundleIdentifier": "com.longtimenoc.missilewars",
      "infoPlist": {
        "NSLocationAlwaysUsageDescription": "This app needs access to your location to alert about nearby items of interest.",
        "UIBackgroundModes": [
          "location",
          "fetch"
        ]

      }
    },
    "android": {
      "permissions": [
        "ACCESS_FINE_LOCATION",
        "ACCESS_BACKGROUND_LOCATION",
        "RECEIVE_BOOT_COMPLETED",
        "SCHEDULE_EXACT_ALARM",
        "com.android.vending.BILLING"
      ],
      "adaptiveIcon": {
        "foregroundImage": "./assets/logo.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.longtimenoc.missilewars"
    },
    "web": {
      "favicon": "./assets/logo.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-notifications",
        {
          "icon": "./assets/logo.png",
          "color": "#ffffff",
          "defaultChannel": "default",
          "sounds": []
        }
      ],
      "expo-secure-store",
      [
        "expo-build-properties",
        {
          "ios": {
            "useFrameworks": "static"
          }
        }
      ],
      [
        "react-native-google-mobile-ads",
        {
          "androidAppId": "ca-app-pub-4035842398612787~8146111264",
          "iosAppId": "ca-app-pub-4035842398612787~7024601286",
          "userTrackingUsageDescription": "This identifier will be used to deliver personalized ads to you."
        }
      ]

    ],
    "owner": "osog",
    "runtimeVersion": {
      "policy": "appVersion"
    },
    "updates": {
      "url": "https://u.expo.dev/250fd3fc-22b2-411d-abfc-63f3a6b81328"
    }
  }
}