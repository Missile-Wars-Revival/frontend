{
  "expo": {
    "name": "missile-wars",
    "slug": "missilewars",
    "scheme": "missile-wars",
    "version": "0.0.1",
    "orientation": "portrait",
    "icon": "./assets/logo.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/logo.png",
      "backgroundColor": "#ffffff"
    },
    "extra": {
      "dotenv": ".env",
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "250fd3fc-22b2-411d-abfc-63f3a6b81328"
      }
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.longtimenoc.misslewars",
      "infoPlist": {
        "NSLocationAlwaysUsageDescription": "This app needs access to your location to alert about nearby items of interest."
      }
    },
    "android": {
      "permissions": [
        "ACCESS_FINE_LOCATION",
        "ACCESS_BACKGROUND_LOCATION"
      ],
      "adaptiveIcon": {
        "foregroundImage": "./assets/logo.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.longtimenoc.misslewars"
    },
    "web": {
      "favicon": "./assets/logo.png"
    },
    "plugins": [
      "expo-router",
      [
        "@rnmapbox/maps",
        {
            "RNMapboxMapsVersion": "= 11.0.0",
            "RNMapboxMapsDownloadToken": "sk.eyJ1Ijoic2xpbWV5Y2FiYmFnZTEyIiwiYSI6ImNseDdsY2gwbzB3ZmYya3M1a2s5ZDFnNWMifQ.JbwCsr8RyFZK9HYRBLFwkg"
        }
    ],
      [
        "expo-location",
        {
          "locationWhenInUsePermission": "Show current location on map."
        }
      ]
    ],
    "owner": "osog",
    "runtimeVersion": "exposdk:51.0.11",
    "updates": {
      "url": "https://u.expo.dev/250fd3fc-22b2-411d-abfc-63f3a6b81328"
    }
  }
}