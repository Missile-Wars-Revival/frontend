{
  "expo": {
    "name": "missle-wars",
    "slug": "missle-wars",
    "scheme": "missle-wars",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/logo.gif",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/logo.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "extra": {
      "dotenv": ".env"
    },
    "assetBundlePatterns": ["**/*"],
    "ios": {
      "supportsTablet": true,
      "config": {
        "googleMapsApiKey": "${GOOGLE_MAPS_API_KEY}",
        "backendUrl": "${BACKEND_URL}",
        "permissions": ["ACCESS_FINE_LOCATION"]
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/logo.gif",
        "backgroundColor": "#ffffff"
      },
      "config": {
        "googleMaps": {
          "googleMapsApiKey": "${GOOGLE_MAPS_API_KEY}"
        },
        "permissions": ["ACCESS_FINE_LOCATION"]
      }
    },
    "web": {
      "favicon": "./assets/logo.png"
    },
    "plugins": ["expo-router"]
  }
}
