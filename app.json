{
  "expo": {
    "name": "missile-wars",
    "slug": "missilewars",
    "scheme": "missile-wars",
    "version": "0.0.1",
    "orientation": "portrait",
    "icon": "./assets/logo.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/logo.png",
      "backgroundColor": "#ffffff"
    },
    "extra": {
      "dotenv": ".env",
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "250fd3fc-22b2-411d-abfc-63f3a6b81328"
      }
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.longtimenoc.misslewars",
      "config": {
        "googleMapsApiKey": "AIzaSyBKb33CQ0Dbt_IeD7fCfaPC5ugigphNcgY" 
      },
      "infoPlist": {
        "UIBackgroundModes": ["location", "fetch"],
        "NSLocationWhenInUseUsageDescription": "This app uses location services to alert you about nearby events.",
        "NSLocationAlwaysUsageDescription": "This app uses location services to alert you about nearby events, even when the app is closed."
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/logo.png",
        "backgroundColor": "#ffffff"
      },
      "permissions": ["ACCESS_FINE_LOCATION", "ACCESS_COARSE_LOCATION", "INTERNET", "RECEIVE_BOOT_COMPLETED"],
      "useNextNotificationsApi": true,
      "package": "com.longtimenoc.misslewars",
      "config": {
        "googleMaps": {
          "apiKey": "AIzaSyBKb33CQ0Dbt_IeD7fCfaPC5ugigphNcgY" 
        }
      }
    },
    "web": {
      "favicon": "./assets/logo.png"
    },
    "plugins": [
      "expo-router"
    ],
    "owner": "osog"
  }
}
